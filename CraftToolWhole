importBefore();

function first(p1) {
{
  a = perkName[p1];
  document.getElementById('selectOne').innerHTML = a;
}  
  perkNameOne = perkName.slice(1, 26);
  perkEffectOne = perkEffect.slice(1, 26);
  perkPercentOne = perkPercent.slice(1, 26);
  document.getElementById('selectedFirst').innerHTML = null;
  importFirst();
  
  document.getElementById("myBtn").addEventListener("click", function() {
    second(1);
  });
}

function second(p1) {
{
  b = perkName[p1];
  document.getElementById('selectTwo').innerHTML = b;
}  
  perkNameTwo = perkName.slice(2, 26);
  perkEffectTwo = perkEffect.slice(2, 26);
  perkPercentTwo = perkPercent.slice(2, 26);
  document.getElementById('selectedSecond').innerHTML = null;
  importSecond();
  
  document.getElementById("myBtn").addEventListener("click", function() {
    third(2);
  });
}

function third(p1) {
{
  c = perkName[p1];
  document.getElementById('selectThree').innerHTML = c;
}  
  perkNameThree = perkName.slice(3, 26);
  perkEffectThree = perkEffect.slice(3, 26);
  perkPercentThree = perkPercent.slice(3, 26);
  document.getElementById('selectedThird').innerHTML = null;
  importThird();
}

